def turn_right():
    turn_left()
    turn_left()
    turn_left()

while not at_goal():
    while right_is_clear():
        while front_is_clear():
            move()
            
        turn_right()
        if front_is_clear():
            move()
    if front_is_clear():
        move()
    else:
        turn_left()
        
